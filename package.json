{
  "name": "EcommerceApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "rm -rf ./node_modules",
    "ios-install": "cd ios && pod install && cd ..",
    "mock": "json-server './src/mock/db.json' --routes './src/mock/routes.json' -m './src/mock/middlewares.js' --config json-server.json",
    "run-ios": "npx react-native run-ios",
    "run-ios-release": "npx react-native run-ios --configuration Release",
    "run-ios-iPhone6": "npx react-native run-ios --simulator='iPhone 6'",
    "run-ios-iPhone11": "npx react-native run-ios --simulator='iPhone 11'",
    "run-android": "npx react-native run-android",
    "run-android-release": "npx react-native run-android --variant release",
    "android-shake": "adb shell input keyevent 82",
    "ngrok": "ngrok http 3000"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.14.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^5.11.10",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "add": "^2.0.6",
    "axios": "^0.21.1",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-fast-image": "^8.3.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-modal": "^11.7.0",
    "react-native-pager-view": "^5.1.7",
    "react-native-reanimated": "1.13.1",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.2.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-tab-view": "^3.0.1",
    "react-native-vector-icons": "^8.1.0",
    "recyclerlistview": "^3.0.5",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/runtime": "^7.13.10",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.2",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
